{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "disks": [
        {
            "name": "my-disk",
            "mountTo": "images/profile"
        }
    ]
  }